{{! muestro los productos en mi bd }}
<div class="title">
  <h1>Productos</h1>
  <div class="options">
    <button id="pageDown">Anterior</button>
    <button id="pageUp">Siguiente</button>
    <button id="page">Pagina</button>
    <button id="limit">Limitar</button>
    <button id="sort">Ordenar</button>
    <button id="query">Categoria</button>
  </div>
</div>
<br />

<div id="productList" action="">
  <div class="container">
    {{#each products}}
        <div class="product-card" id="products-container">
          <h3>{{this.title}}</h3>
          <hr />
          <p>Id: {{this._id}}</p>
          <p>Descripci√≥n: {{this.description}}</p>
          <p>Precio: {{this.price}}</p>
          <p>Stock: {{this.stock}}</p>
          <p>Codigo: {{this.code}}</p>
          <p>Categoria: {{this.category}}</p>
          <p>Imagen: {{this.thumbnails}}</p>
        </div>
    {{/each}}
  </div>
</div>

<script src="/js/home.js"></script>